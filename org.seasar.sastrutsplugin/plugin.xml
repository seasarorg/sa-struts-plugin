<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.popupMenus">
      <viewerContribution
            id="org.seasar.sastrutsplugin.viewerContribution"
            targetID="#CompilationUnitEditorContext">
         <action
               class="org.seasar.sastrutsplugin.action.OpenJspAction"
               definitionId="org.seasar.sastrutsplugin.command"
               id="org.seasar.sastrutsplugin.OpenJsp"
               label="%JSPFileOpenAction.label"
               menubarPath="additions">
         </action>
      </viewerContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.seasar.sastrutsplugin.category"
            name="SAStruts Plugin">
      </category>
      <command
            categoryId="org.seasar.sastrutsplugin.category"
            id="org.seasar.sastrutsplugin.command"
            name="%JSPFileOpenAction.label">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.seasar.sastrutsplugin.command"
            contextId="org.eclipse.jdt.ui.javaEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            id="org.seasar.sastrutsplugin.editorContribution"
            targetID="org.eclipse.jdt.ui.CompilationUnitEditor">
         <action
               class="org.seasar.sastrutsplugin.action.OpenJspAction"
               definitionId="org.seasar.sastrutsplugin.command"
               id="org.seasar.sastrutsplugin.action.OpenJspAction"
               label="%JSPFileOpenAction.label">
         </action>
      </editorContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.seasar.sastrutsplugin.preference.SAStrutsPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            adaptable="true"
            class="org.seasar.sastrutsplugin.property.SAStrutsPropertyPage"
            id="org.seasar.sastrutsplugin.property.SAStrutsPropertyPage"
            name="%plugin.brand"
            objectClass="org.eclipse.core.resources.IProject">
      </page>
   </extension>



</plugin>
